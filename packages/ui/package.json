{
  "name": "@roxie/ui",
  "version": "1.9.1",
  "description": "The styles of Roxie, based on Bootstrap with many themes",
  "author": "huytrongnguyen <huynguyen8505@gmail.com> (https://github.com/huytrongnguyen)",
  "license": "MIT",
  "main": "dist/js/roxie-ui.js",
  "typings": "dist/js/roxie-ui.d.ts",
  "scripts": {
    "build": "tsc && sass --style=compressed --no-source-map sass:dist/css",
    "postbuild": "cp -r node_modules/font-awesome/fonts dist",
    "watch": "sass --style=compressed --no-source-map --watch sass:dist/css"
  },
  "dependencies": {
    "bootstrap": "4.3.1",
    "c3": "0.7.10",
    "font-awesome": "4.7.0",
    "jquery": "3.4.1",
    "popper.js": "1.15.0",
    "toastr": "2.1.4"
  },
  "devDependencies": {
    "sass": "1.23.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "sass",
    "LICENSE"
  ]
}

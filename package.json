{
  "name": "roxie",
  "version": "0.0.3",
  "description": "JavaScript library that build on top of React",
  "main": "dist/index.js",
  "author": "Lionel Nguyen <huynguyen8505@gmail.com> (https://github.com/huytrongnguyen)",
  "license": "MIT",
  "repository": "https://github.com/huytrongnguyen/roxie",
  "bugs": "https://github.com/huytrongnguyen/extension-react/issues",
  "scripts": {
    "start": "babel-node server.js --presets env --plugins transform-decorators-legacy",
    "start:dev": "nodemon --watch server.js --watch src -e jsx,js,json --exec yarn start",
    "build": "rm -rf dist/* && babel src -D -d dist",
    "prepublish": "yarn build"
  },
  "files": [
    "LICENSE",
    "dist/"
  ],
  "babel": {
    "presets": [ "env", "react" ],
    "plugins": [
      "transform-class-properties",
      "transform-decorators-legacy",
      "transform-object-rest-spread",
      ["root-import", [
          { "rootPathSuffix": "src/" },
          { "rootPathPrefix": "@", "rootPathSuffix": "docs/src/" },
          { "rootPathPrefix": "#", "rootPathSuffix": "example/src/" }
      ]]
    ],
    "ignore": "node_modules"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "express": "^4.16.3",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "rxjs": "^6.2.2"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-plugin-root-import": "6.1.0",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-decorators-legacy": "1.3.5",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "6.24.1",
    "nodemon": "1.18.3",
    "yarn": "1.9.4"
  }
}

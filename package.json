    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "1.7.0",
    "babelify": "8.0.0",
    "browserify": "16.2.2",
    "express": "4.16.3",
    "gulp": "3.9.1",
    "gulp-sass": "4.0.1",
    "gulp-streamify": "1.0.2",
    "gulp-uglify": "3.0.0",

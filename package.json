{
  "name": "roxie",
  "version": "1.0.10",
  "description": "JavaScript library that build on top of React",
  "author": "Lionel Nguyen <huynguyen8505@gmail.com> (https://github.com/huytrongnguyen)",
  "license": "MIT",
  "repository": "https://github.com/huytrongnguyen/roxie",
  "bugs": "https://github.com/huytrongnguyen/roxie/issues",
  "devDependencies": {
    "@types/express": "4.16.1",
    "@types/jest": "24.0.13",
    "@types/lodash": "4.14.123",
    "@types/node": "11.12.2",
    "@types/react": "16.8.10",
    "@types/react-dom": "16.8.3",
    "jest": "24.8.0",
    "lerna": "3.13.1",
    "ts-jest": "24.0.2",
    "typescript": "3.4.1",
    "yarn": "1.15.2"
  },
  "jest": {
    "verbose": true,
    "collectCoverage": true,
    "moduleFileExtensions": [
      "js",
      "ts",
      "tsx",
      "json",
      "node"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "(.test.ts|.test.tsx|.test.js)$"
  },
  "scripts": {
    "publish": "lerna run build && lerna publish",
    "test": "jest"
  }
}

{
  "name": "roxie",
  "version": "1.2.0",
  "description": "JavaScript library that build on top of React",
  "author": "Lionel Nguyen <huynguyen8505@gmail.com> (https://github.com/huytrongnguyen)",
  "license": "MIT",
  "repository": "https://github.com/huytrongnguyen/roxie",
  "bugs": "https://github.com/huytrongnguyen/roxie/issues",
  "devDependencies": {
    "@types/react-highlight.js": "1.0.0",
    "@types/jest": "24.0.18",
    "@types/jquery": "3.3.31",
    "@types/node": "12.7.5",
    "@types/react": "16.9.2",
    "@types/react-dom": "16.9.0",
    "@types/react-router-dom": "4.3.5",
    "express": "4.17.1",
    "jest": "24.9.0",
    "react-highlight.js": "1.0.7",
    "lerna": "3.16.4",
    "parcel": "1.12.3",
    "react-router-dom": "5.0.1",
    "sass": "1.22.12",
    "ts-jest": "24.0.2",
    "typescript": "3.6.3",
    "yarn": "1.17.3"
  },
  "alias": {
    "@roxie/core": "./packages/core/lib",
    "@roxie/components": "./packages/components/lib",
    "@roxie/ui": "./packages/ui/lib"
  },
  "jest": {
    "verbose": true,
    "collectCoverage": true,
    "moduleFileExtensions": [
      "js",
      "ts",
      "tsx",
      "json",
      "node"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "(.test.ts|.test.tsx|.test.js)$"
  },
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "build": "lerna run build",
    "publish": "lerna publish",
    "build-docs": "parcel build docs/internal/index.html --no-source-maps --no-content-hash --no-autoinstall --out-dir docs --public-url ./roxie/",
    "start-docs": "node server.js",
    "test": "jest"
  },
  "dependencies": {
    "react": "^16.9.0",
    "react-dom": "^16.9.0"
  }
}

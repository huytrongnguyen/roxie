{
  "name": "roxie",
  "version": "0.0.1",
  "description": "Utility Components and Services for React, Angular",
  "main": "dist/index.js",
  "author": "Lionel Nguyen <huynguyen8505@gmail.com> (https://github.com/huytrongnguyen)",
  "license": "MIT",
  "repository": "https://github.com/adtonline/roxie",
  "scripts": {
    "start": "babel-node example/src/server.js --presets env --plugins transform-decorators-legacy --content ./example/dist --port 3000",
    "build": "rm -rf dist/* && babel src -D -d dist",
    "prepublish": "yarn build"
  },
  "files": [
    "LICENSE",
    "dist/"
  ],
  "babel": {
    "presets": [
      "env",
      "react"
    ],
    "plugins": [
      "transform-class-properties",
      "transform-decorators-legacy",
      "transform-object-rest-spread",
      ["root-import", [{
          "rootPathSuffix": "src/"
          }, {
          "rootPathPrefix": "@",
          "rootPathSuffix": "docs/src/"
          }, {
            "rootPathPrefix": "#",
            "rootPathSuffix": "example/src/"
        }]
      ]
    ],
    "ignore": "node_modules"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-plugin-root-import": "6.1.0",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-decorators-legacy": "1.3.5",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "6.24.1",
    "express": "4.16.3",
    "yarn": "1.7.0"
  }
}

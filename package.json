{
  "name": "roxie",
  "version": "0.0.10",
  "description": "JavaScript library that build on top of React",
  "main": "dist/index.js",
  "author": "Lionel Nguyen <huynguyen8505@gmail.com> (https://github.com/huytrongnguyen)",
  "license": "MIT",
  "repository": "https://github.com/huytrongnguyen/roxie",
  "bugs": "https://github.com/huytrongnguyen/roxie/issues",
  "scripts": {
    "prestart": "yarn prepare",
    "start": "NODE_ENV=production node ./docs/index.js",
    "prepare": "yarn build",
    "prebuild": "rm -rf dist/*",
    "build": "babel src -D -d dist"
  },
  "files": [
    "LICENSE",
    "dist/"
  ],
  "babel": {
    "presets": ["@babel/preset-env", "@babel/preset-react"],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      ["@babel/plugin-proposal-decorators", { "legacy": true }],
      "@babel/plugin-proposal-object-rest-spread",
      ["root-import", { "rootPathSuffix": "src" }]
    ],
    "ignore": ["node_modules"]
  },
  "peerDependencies": {
    "react": ">=16.3.2",
    "react-dom": ">=16.3.3"
  },
  "dependencies": {
    "@babel/polyfill": "7.0.0",
    "axios": "0.18.0",
    "express": "4.16.3",
    "moment": "2.22.2",
    "rxjs": "6.3.2"
  },
  "devDependencies": {
    "@babel/cli": "7.0.0",
    "@babel/core": "7.0.0",
    "@babel/register": "7.0.0",
    "@babel/plugin-proposal-class-properties": "7.0.0",
    "@babel/plugin-proposal-decorators": "7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0",
    "@babel/polyfill": "7.0.0",
    "@babel/preset-env": "7.0.0",
    "@babel/preset-react": "7.0.0",
    "babel-plugin-root-import": "6.1.0",
    "axios": "0.18.0",
    "express": "4.16.3",
    "moment": "2.22.2",
    "react": "16.3.2",
    "react-dom": "16.3.3",
    "rxjs": "6.3.2",
    "yarn": "1.9.4"
  }
}
